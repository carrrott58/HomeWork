from itertools import permutations

t="1345 2367 31267 4156 5147 6234 7235"
g="АБВГ БАЖД ВАГЕ ГАВД ДБЖГЕ ЕЖДВ ЖДБЕ"

g = {x[0]: set(x[1:]) for x in g.split()}

print(*"1234567")

for p in permutations("АБВГДЕЖ"):
    new_g=t
    for x,y in zip("1234567", p):
        new_g=new_g.replace(x,y)
    new_g={x[0]: set(x[1:]) for x in new_g.split()}
    if new_g==g:
        print(*p)

#19
